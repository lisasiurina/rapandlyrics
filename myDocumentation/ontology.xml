    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Pangrammatic -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Pangrammatic">
