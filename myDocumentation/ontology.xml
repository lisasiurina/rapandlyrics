     xml:base="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:pimpmyrap="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#definition -->


    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#IsPartOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#IsPartOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSong"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album"/>
        <rdfs:comment>property that connects class MusicWork to the class Album.</rdfs:comment>
        <rdfs:label>isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasAlbum -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasAlbum">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album"/>
        <rdfs:comment>property that connects classes of the ontology to the class Album.</rdfs:comment>
        <rdfs:label>hasAlbum</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasGenre -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasGenre">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Genre"/>
        <rdfs:comment>property that connects classes of the ontology to the music genre.</rdfs:comment>
        <rdfs:label>hasGenre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasLyrics -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasLyrics">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Lyrics"/>
        <rdfs:comment>property that connects classes of the ontology to the Lyrics.</rdfs:comment>
        <rdfs:label>hasLyrics</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasPlace -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasPlace">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Place"/>
        <rdfs:comment>property that connects classes of the ontology to the class Place</rdfs:comment>
        <rdfs:label>hasPlace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSentiment -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSentiment">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Sentiment"/>
        <rdfs:comment>property that connects classes of the ontology to the class Sentiment.</rdfs:comment>
        <rdfs:label>hasSentiment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSong -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSong">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:comment>property that connects classes of the ontology to the class Song.</rdfs:comment>
        <rdfs:label>hasSong</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSubject -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSubject">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#isReferencedIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Topic"/>
        <rdfs:comment>property that connects classes of the ontology to the class Topic.</rdfs:comment>
        <rdfs:label>hasTopic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasTimeSpan -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasTimeSpan">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Time_Span"/>
        <rdfs:comment>property that connects classes of the ontology to the class Time Span.</rdfs:comment>
        <rdfs:label>hasTimeSpan</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#isReferencedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#isReferencedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#mentionsTopic"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:comment>property that connects the class Topic to the class Person.</rdfs:comment>
        <rdfs:label>isReferencedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#isReferencedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#isReferencedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Topic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:comment>property that connects class Topic to the class Person.</rdfs:comment>
        <rdfs:label>isReferencedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#mentionsTopic -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#mentionsTopic">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Topic"/>
        <rdfs:comment>property that connects classes of the ontology to the class Topic.</rdfs:comment>
        <rdfs:label>mentionsTopic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#references -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#references">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:comment>The object property that connects class Person to the same class Person.</rdfs:comment>
        <rdfs:label>references</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#usesLiteraryDevice -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#usesLiteraryDevice">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice"/>
        <rdfs:comment>property that connects the class Person to the class Literary Device</rdfs:comment>
        <rdfs:label>usesLiteraryDevice</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->


    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    


    <!-- 
