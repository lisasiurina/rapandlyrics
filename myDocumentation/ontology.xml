    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://dbpedia.org/ontology/City -->


    <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/City">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>dbo:City</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://dbpedia.org/ontology/Country -->


    <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/Country">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Country"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>dbo:City</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/elements/1.1/title">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>dc:title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSentimentScore -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#hasSentimentScore">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#mo:activity_start -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#mo:activity_start">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicArtist"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/maker -->


    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/maker">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicalWork"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>foaf:maker</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/member -->


    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/member">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/name -->


    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/ontology/mo/Genre -->


    <owl:Class rdf:about="http://purl.org/ontology/mo/Genre">
        <rdfs:label>Genre</rdfs:label>
        <skos:definition>An expressive style of music.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Album">
        <rdfs:label>Album</rdfs:label>
        <skos:definition>An album is a collection of audio recordings issued as a collection on compact disc (CD), vinyl, audio tape, or another medium.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#BMW -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#BMW">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Car"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Mercedes-Benz"/>
        <rdfs:label>BMW</rdfs:label>
        <skos:definition>BMW is defined as an acronym for Bayerische Motoren Werke AG, or in English, Bavarian Motor Works.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Car -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Car">
        <rdfs:label>Car</rdfs:label>
        <skos:definition>a road vehicle.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#City -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#City">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Place"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Country"/>
        <rdfs:label>City</rdfs:label>
        <skos:definition>A relatively large and permanent settlement, particularly a large urban settlement</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Country -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Country">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Place"/>
        <rdfs:comment>The country where the thing is located.</rdfs:comment>
        <rdfs:label>Country</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#East_Coast -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#East_Coast">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Rap"/>
        <rdfs:label>East Coast Rap</rdfs:label>
        <skos:definition>A regional genre of hip hop music that encompasses any artists or music that originated in the East Coast region of the United States.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Imagery -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Imagery">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice"/>
        <rdfs:label>Imagery</rdfs:label>
        <skos:definition>Imagery is the literary term used for language and description that appeals to our five senses. When a music artist attempts to describe something so that it appeals to our sense of smell, sight, taste, touch, or hearing; he/she has used imagery.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Irony -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Irony">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice"/>
        <rdfs:label>Irony</rdfs:label>
        <skos:definition>The use of irony in literature refers to playing around with words such that the meaning implied by a sentence or word is actually different from the literal meaning. Often irony is used to suggest the stark contrast of the literal meaning being put forth. The deeper, real layer of significance is revealed not by the words themselves but the situation and the context in which they are placed.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice">
        <rdfs:label>LiteraryDevice</rdfs:label>
        <skos:definition>literary device is a technique a writer uses to produce a special effect in their writing.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Litote -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Litote">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice"/>
        <rdfs:label>Litote</rdfs:label>
        <skos:definition>Litotes are a figure of speech consisting of an understatement in which an affirmative is expressed via the negative of its contrary.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Lyrics -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Lyrics">
        <rdfs:label>Lyrics</rdfs:label>
        <skos:definition>Associates lyrics with a musical work</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Mercedes-Benz -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Mercedes-Benz">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Car"/>
        <rdfs:label>Mercedes-Benz</rdfs:label>
        <skos:definition>Mercedes-Benz is a German global automobile marque and a division of Daimler AG. The brand is known for luxury vehicles, buses, coaches, and trucks.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Metonymy -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Metonymy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice"/>
        <rdfs:label>Metonomy</rdfs:label>
        <skos:definition>A metonymy is a word or phrase that is substituted for a more formal word or phrase to which it is related.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicArtist -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicArtist">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Person"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#MusicGroup"/>
        <rdfs:label>Music Artist</rdfs:label>
