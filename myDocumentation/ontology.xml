    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Simile -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Simile">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#LiteraryDevice"/>
        <rdfs:label>Similie</rdfs:label>
        <skos:definition>A simile is where one thing is compared to another to help with describing something.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Spoonerism -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Spoonerism">
