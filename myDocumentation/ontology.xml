        <rdfs:label>Synecdoche</rdfs:label>
        <skos:definition>When a part of something stands in for the whole thing OR the whole thing stands in for the part.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Synesthesia -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Synesthesia">
