        <rdfs:label>Spoonerism</rdfs:label>
        <skos:definition>A spoonerism is a play on words in which the sounds of two or more words are transposed.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Synecdoche -->


    <owl:Class rdf:about="http://www.semanticweb.org/lisa/ontologies/2019/7/pimpmyrap#Synecdoche">
